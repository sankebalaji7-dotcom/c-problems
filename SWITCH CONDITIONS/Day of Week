#include <stdio.h>
int main() {
    int week;

    scanf("%d",&week);

    switch(week){
     case 1:
        printf("Monday");
        break;
     case 2:
        printf("Tuesday");
        break;
     case 3:
        printf("wednesday");
        break;
     case 4:
        printf("thursday");
        break;
     case 5:
        printf("Friday");
        break;
     case 6:
        printf("satrday");
        break;
        case 7:
         printf("sunday");
        default:
        printf("nivalid week");

    }
    
    return 0;
}
