#include <stdio.h>
int main() {
    char vowles;
    scanf("%c",&vowles);
    
    switch(vowles){
        case 'A':
        case 'E':
        case 'I':
        case 'O':
        case 'U':
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
        printf("Vowles");
        break;

        default:
        printf("invalid vowles (a e i o u)");
    }
    return 0;
}
