#include <stdio.h>
int main() {
    char ch;

    printf("Input:");
    scanf("%c",&ch);

    if(ch>='0' && ch<='9'){
        printf("Digit");
    }
    else if((ch>='a' && ch<='z' )|| (ch>='A' && ch<='Z')){

    printf("alphabade");
    }
    else{
        printf("specail character");
    }
    return 0;
}
